  background: linear-gradient(to right, #ffffff, #fffceb) !important;
}
    #campaign_sign {
      background-color: #f44336 !important;  /* jelly‑bean yellow */
      border-color:     #ffeb3b !important;  /* jelly‑bean red */

	
    `;

    var styleElem = document.createElement('style');
    styleElem.type = 'text/css';
    
    styleElem.appendChild(document.createTextNode(css));
    
    document.head.appendChild(styleElem);
}

addDynamicCSS();


